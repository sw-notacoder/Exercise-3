cust_id1 <- runif(100, min=0, max=100)
View(cust_id1)
cust_id<- as.integer(cust_id1)
View(cust_id)
income <- rnorm(100, mean=250000, sd=75000)
View(income)
income_int<- as.integer(income)
gen_temp<- c(1:100)
gen_temp1<- sample(gen_temp)
gender <- ifelse(gen_temp<=60,"M","F")
cust_df<- data.frame(cust_id,income_int,gender)
View(cust_df)
